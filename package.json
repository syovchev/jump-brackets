{
	"name": "jump-brackets",
	"displayName": "jump-brackets",
	"description": "Navigate between brackets.",
	"version": "0.0.1",
	"publisher": "StAlYo",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"keybindings": [
			{
				"command": "jump-forward",
				"key": "alt+'"
			},
			{
				"command": "jump-backwards",
				"key": "alt+;"
			},
			{
				"command": "jump-forward-selection",
				"key": "shift+alt+'"
			},
			{
				"command": "jump-backwards-selection",
				"key": "shift+alt+;"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/syovchev/jump-brackets"
	}
}
